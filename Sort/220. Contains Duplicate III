class Solution {
    public boolean containsNearbyAlmostDuplicate(int[] nums, int k, int t) {
        
        for(int i = 0;i<nums.length;i++){
            for(int j = i+1;j<nums.length && j<=i+k;j++){
                System.out.println(Math.abs(nums[i]-nums[j]));
                if(Math.abs(1L * nums[i] - nums[j]) <= t)
                    return true;
            }
        }
        return false;
    }
}
