class Solution {
    public int hammingDistance(int x, int y) {
        int xorNum = x ^ y;
        return Integer.bitCount(xorNum);
        //return countSetBits(xorNum);
    }
    
    //Doesn't work for large input
    public int countSetBits(int num) {
        int arr[] = new int[num+1];
        for(int i = 1;i<=num;i++){
            arr[i] = arr[i>>1] + i%2;
            //System.out.println(arr[i]);
        }
        return arr[num];
    }
}
