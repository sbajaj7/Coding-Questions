class Solution {
    int count = 0;
    public int countNodes(TreeNode root) {
        if(root == null)
            return 0;
        
        //countNodesHelper(root);
        count = computeNodeCount(root);
        return count;
    }
    public void countNodesHelper(TreeNode root){
        if(root == null)
            return;
        
        count++;
        countNodesHelper(root.left);
        countNodesHelper(root.right);
    }
}
